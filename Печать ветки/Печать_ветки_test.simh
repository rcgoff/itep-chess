;файл вывода результатов
;! del П3.prn
;att lpt П3.prn

;отключить печать статистики по командам
de PRINT_SYS_STAT 0

;режим ИТЭФ (команда ФА)
de ITEP_MODE 1

;отображение 8 старших адресов на регистры
de MOSU_MODE 2

;включить трассировку (печать регистров после каждой команды)
de DEBUG_DUMP_REGS 1
de DEBUG_DUMP_MEM 1
! del Печать_ветки_debug.txt
set console debug=Печать_ветки_debug.txt
set cpu debug


;загрузка программы и библиотеки Б-61
load Печать_ветки.m20
load ../b61.m20

;РА:=уровень в массивах
de 7776 000000000030000

;текущий уровень в дереве игры
de 1700 000000000050000

;вет0/0+1
;de 4067 000000012340000

;мгновенный возврат на месте неготовой програмы Пq2
de 7752 056000000070000

;записать в Omega стоп
de 0007 777777777777777

;запуск с "ветка 2"
run 4625

;выход
exit
