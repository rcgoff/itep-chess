;Тест программы П3 (размещена в файле П3.m20)
;Л.Ядренников 10.01.2022

;файл вывода результатов
! del П3.prn
att lpt П3.prn

;отключить печать статистики по командам
de PRINT_SYS_STAT 0

;включить трассировку (печать регистров после каждой команды)
de DEBUG_DUMP_REGS 1
de DEBUG_DUMP_MEM 1
! del П3_debug.txt
set console debug=П3_debug.txt
set cpu debug


;исходное машинное слово в alpha-печ
de 0025 123456776543210
de 0025 111111111111111

;уровень 11oct=09dec в n
;de 1700 000000000110000

;программа вывода одного слова из gamma-печ на десятичную печать 
de 2000 050010007770026
de 2010 070002600000000
de 2020 777777777777777

;загрузка программы и библиотеки Б-61
load П3.m20
load b61.m20

;записать в Omega стоп
de 0007 777777777777777

;запуск П3
run 4400

;запуск печати
run 2000

;выход
exit
