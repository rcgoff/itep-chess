;Тест программы А (размещена в файле av4.m20)
;Л.Ядренников 10.01.2022

;файл вывода результатов
! del out.prn
att lpt out.prn

;отключить печать статистики по командам
de PRINT_SYS_STAT 0

;включить трассировку (печать регистров после каждой команды)
de DEBUG_DUMP_REGS 1
de DEBUG_DUMP_MEM 1
! del av4_debug.txt
set console debug=av4_debug.txt
set cpu debug


;исходное машинное слово в αпеч
de 0025 123456776543210

;программа вывода одного слова из γпеч на десятичную печать 
de 0300 050010007770026
de 0301 070002600000000
de 0302 777777777777777

;загрузка программы A
load av4.m20

;записать в Ω стоп
de 0007 777777777777777

;запуск A
run 200

;запуск печати
run 300

;выход
exit
