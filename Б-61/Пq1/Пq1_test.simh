;Тест программы Пq1 и 2->10 (размещены в файле Пq1.m20)
;Л.Ядренников 22.05.2022

;отключить печать статистики по командам
de PRINT_SYS_STAT 0

;включить трассировку (печать регистров после каждой команды)
de DEBUG_DUMP_REGS 1
de DEBUG_DUMP_MEM 1
;de ARITHMETIC_OP_DEBUG 1
! del pq1_debug.txt
set console debug=pq1_debug.txt
set cpu debug
set lpt debug

;файл вывода результатов
! del out.prn
att lpt out.prn

;обращение к Пq1 для вывода слов с адресами 1000..1014 
;(слова содержатся в файле numbers.m20) 
de 2000 016200277510007
de 2001 000100000001014
de 2002 777777777777777

;загрузка чисел
load numbers.m20

;загрузка программы A
load Пq1.m20

;запуск Пq1
run 2000

;проверка правильности работы, если a1>an
de 2001 000101300001000
run 2000

;проверка правильности работы, если a1=an
de 2001 000101000001010
run 2000

;выход
exit
